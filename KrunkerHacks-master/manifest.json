{
  "name": "Krunker Hacks",
  "homepage_url": "https://github.com/skidlamer/krunker_hero/archive/master.zip",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Krunker Hack Made By Uglyz",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://krunker.io/*",
    "*://matchmaker.krunker.io/*",
    "background"
  ],
  "web_accessible_resources": [
    "zip.js", "zip-ext.js"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Krunekr Hacks"
  },
  "background": {
    "scripts": ["hook.js"]
  }
}
